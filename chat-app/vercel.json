{
  "rewrites": [
    {
      "source": "/api/users/(.*)",
      "destination": "https://wechat.gt.tc/api/users/$1"
    },
    {
      "source": "/api/chat/(.*)",
      "destination": "https://wechat.gt.tc/api/chat/$1"
    },
    {
      "source": "/api/mail/(.*)",
      "destination": "https://wechat.gt.tc/api/mail/$1"
    }
  ]
}
